{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution":"Node",
    "resolveJsonModule":true,
    "downlevelIteration":true,
  },
  "include": ["src/**/*"]
}
